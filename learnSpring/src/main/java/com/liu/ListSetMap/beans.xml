<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
     http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 设置注入 -->
	<bean id="food1" class="com.liu.ListSetMap.Food">
		<property name="name" value="水果" />
		<property name="list">
			<list>
				<value>橘子</value>
				<value>柚子</value>
				<value>苹果</value>
			</list>
		</property>
		<property name="set">
			<set>
				<value>葡萄</value>
				<value>香蕉</value>
			</set>
		</property>
		<property name="map">
			<map>
				<entry key="数学" value="90" />
				<entry key="英语" value="99" />
				<entry key="语文" value="100" />
			</map>
		</property>
		<property name="prop">
			<props>
				<prop key="血压">正常</prop>
				<prop key="身高">175</prop>
			</props>
		</property>
		<property name="array">
			<list>
				<value>计算机导论</value>
				<value>生命科学导论</value>
			</list>
		</property>
	</bean>

	<!-- 构造注入 -->
	<bean id="food2" class="com.liu.ListSetMap.Food">
	</bean>
</beans>